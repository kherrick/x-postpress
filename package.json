{
  "name": "x-postpress",
  "version": "1.2.5",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/kherrick/x-postpress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kherrick/x-postpress.git"
  },
  "main": "./build/esm-bundled/src/components/x-postpress.js",
  "scripts": {
    "build:prpl-server": "./bin/build-prpl-server",
    "build:static": "./bin/build-static",
    "build": "npm run lint && npm run build:prpl-server && npm run build:static",
    "build-for-github-pages": "BASE_PATH='/x-postpress/' npm run build",
    "deploy": "gh-pages --dotfiles --dist ./build/es5-bundled/",
    "eslint-fix": "npx -q eslint --fix ./src ./test",
    "eslint": "npx -q eslint ./src ./test",
    "lint": "npm run eslint && npm run polymer-lint",
    "polymer-lint-fix": "npx -q polymer lint --fix --sources \"./src/**/*\"",
    "polymer-lint": "npx -q polymer lint --sources \"./src/**/*\"",
    "serve:prpl-server": "npx -q prpl-server --root server/build",
    "serve:static": "npx -q polymer serve --port 5000 build/es5-bundled",
    "start": "npx -q polymer serve --compile always",
    "test:integration": "npx -q mocha ./test/integration --timeout=15000",
    "test:regenerate_screenshots": "./bin/ci-regenerate-screenshots",
    "test:sauce": "npx -q polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:unit": "npx -q polymer test --module-resolution=node --npm",
    "test": "./bin/ci"
  },
  "dependencies": {
    "@polymer/lit-element": "^0.6.5",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "fetch-polyfill": "^0.8.2",
    "lit-html": "^1.0.0-rc.1",
    "pwa-helpers": "^0.9.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "fetch-mock": "^7.2.7",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prettier": "^1.15.3",
    "prpl-server": "^1.3.0",
    "puppeteer": "^1.5.0",
    "wct-mocha": "^1.0.0"
  }
}
