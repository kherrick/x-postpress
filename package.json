{
  "name": "x-postpress",
  "version": "3.0.0-alpha.5",
  "description": "A Web Component that fetches WordPress posts from the REST API based on attributes set on the element.",
  "author": "x-postpress",
  "main": "dist/umd/module.js",
  "module": "dist/esm/module.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kherrick/x-postpress.git"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:development": "NODE_ENV=development rollup -c",
    "build:service-worker": "workbox generateSW ./service-worker/workbox-config.js",
    "prepublishOnly": "npm run build",
    "start": "es-dev-server --app-index dev/index.html --node-resolve --open --watch",
    "serve": "serve -s . -p 3000",
    "test": "karma start --single-run",
    "test:screenshots-compare": "mocha ./tests/screenshots/compare.js --timeout=30000",
    "test:screenshots-generate": "mocha ./tests/screenshots/generate.js --timeout=30000"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "date-fns": "^2.2.1",
    "lit-element": "^2.2.1",
    "lodash-es": "^4.17.15",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "es-dev-server": "^1.18.0",
    "glob": "^7.1.4",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.2",
    "lit-html": "^1.1.2",
    "mocha": "^6.2.1",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "polyserve": "^0.27.15",
    "puppeteer": "^1.20.0",
    "rollup": "^1.24.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "serve": "^11.1.0",
    "typescript": "^3.6.4",
    "workbox-cli": "^4.3.1"
  },
  "license": "AGPL-3.0",
  "keywords": [
    "Web Components",
    "Custom Elements",
    "TypeScript",
    "WordPress",
    "REST API",
    "lit-html",
    "LitElement"
  ]
}
