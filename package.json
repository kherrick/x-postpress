{
  "name": "x-postpress",
  "version": "2.1.1",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/kherrick/x-postpress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kherrick/x-postpress.git"
  },
  "main": "./build/esm-bundled/src/components/x-postpress.js",
  "scripts": {
    "build:app": "./bin/build-app && npx gulp prpl-server",
    "build:gh-pages:serve:local": "npm run build:gh-pages && npm run serve:app:github",
    "build:gh-pages": "BASE_PATH='/x-postpress/' npm run build:app && cd build && ln -s es5-bundled x-postpress && cd -",
    "build:library": "./bin/build-library && npx gulp prpl-server",
    "build": "npm run build:app",
    "deploy": "npx gh-pages --dotfiles --dist ./build/es5-bundled/",
    "eslint-fix": "npx -q eslint --fix ./src ./test",
    "eslint": "npx -q eslint ./src ./test",
    "lint": "npm run eslint && npm run polymer-lint",
    "lint-fix": "npm run eslint-fix && npm run polymer-lint-fix",
    "polymer-lint-fix": "npx -q polymer lint --fix --sources \"./src/**/*\"",
    "polymer-lint": "npx -q polymer lint --sources \"./src/**/*\"",
    "prepublishOnly": "npm run build:library",
    "serve:app:github": "npx serve build # make sure to run `npm run build:gh-pages` first",
    "serve:app": "npx -q polymer serve --hostname 0.0.0.0 --port 5000 build/es5-bundled",
    "serve:examples": "npx serve examples",
    "serve:prpl-server": "npx -q prpl-server --root server/build",
    "start": "gulp serve",
    "test:integration": "npm run typescript && npm run lint-fix && npx -q mocha ./test/integration --timeout=30000",
    "test:regenerate-screenshots": "./bin/ci-regenerate-screenshots",
    "test:sauce": "npm run typescript && npm run lint-fix && npx -q polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:unit": "npm run typescript && npm run lint-fix && npx -q polymer test --module-resolution=node --npm",
    "test": "./bin/ci",
    "typescript": "./bin/log 'info' '(TypeScript) Compiling app...' && npx tsc"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.2",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "fetch-polyfill": "^0.8.2",
    "lit-element": "^2.0.0-rc.5",
    "lit-html": "^1.0.0-rc.2",
    "pwa-helpers": "^0.9.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.1.2",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fetch-mock": "^7.3.0",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.5",
    "polyserve": "^0.27.15",
    "prettier": "^1.16.1",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.11.0",
    "serve": "^10.1.1",
    "typescript": "^3.2.4",
    "wct-mocha": "^1.0.0"
  },
  "types": "src/types",
  "keywords": [
    "Web Components",
    "Custom Elements",
    "TypeScript",
    "WordPress",
    "REST API",
    "lit-html",
    "LitElement",
    "Progressive Web App",
    "PWA",
    "service worker"
  ]
}
