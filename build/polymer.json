{
  "entrypoint": "index.html",
  "shell": "src/app.js",
  "fragments": [],
  "sources": [
    "images/**/*",
    ".nojekyll",
    "robots.txt",
    "index.html",
    "src/app.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/fetch-polyfill/fetch.js"
  ],
  "builds": [
    {
      "name": "uncompiled-unbundled",
      "browserCapabilities": [
        "es2018",
        "modules",
        "push"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "uncompiled-unbundled"
    },
    {
      "name": "esm-bundled",
      "basePath": "/",
      "browserCapabilities": [
        "es2015",
        "modules",
        "serviceworker"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": "es2015",
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    },
    {
      "name": "es6-bundled",
      "basePath": "/",
      "browserCapabilities": [
        "es2015",
        "serviceworker"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "basePath": "/",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": {
        "inlineCss": true,
        "inlineScripts": true,
        "rewriteUrlsInTemplates": true,
        "sourcemaps": true,
        "stripComments": true
      },
      "addServiceWorker": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ],
    "warningsToIgnore": [
      "non-literal-import",
      "potential-element-naming-issue",
      "undefined-elements"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}